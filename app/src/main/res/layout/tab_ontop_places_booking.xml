<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nguyenvanphituoc.foody.Activity.Fragment.TabPlacesBookingFragment"
    android:background="@color/clGrey">
    <ListView
        android:id="@+id/listvTabPlacesBooking"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_above="@+id/btnTabPlacesBooking"
        android:scrollbarStyle="insideInset"
        android:scrollbars="vertical"/>
    <Button
        android:id="@+id/btnTabPlacesBooking"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        style="?attr/buttonBarButtonStyle"
        android:textColor="@color/clBlack"
        android:text="@string/ACTION_CANCEL_VI"/>
</RelativeLayout>
